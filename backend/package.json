{
  "name": "church-presenter",
  "version": "1.0.0",
  "description": "Church Presenter",
  "main": "build/index.js",
  "scripts": {
    "test": "npx ts-mocha ./**/*.spec.ts --recursive src/test --no-parallel --exit",
    "clean": "npx shx mkdir -p build && npx shx rm -rf build/*",
    "compile": "npm run clean && tsc",
    "serve": "electron build/index.js --onlyApi false",
    "serve-dev": "electron build/index.js --onlyApi true --serverPort 3001",
    "start": "electron-forge start",
    "start-quick": "electron build/index.js",
    "build": "npm run compile && npm run build:frontend",
    "build:frontend": "cd ../frontend && npm run build",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "keywords": [],
  "author": "Jesse Govindan",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.2",
    "commons": "file:../commons",
    "electron-squirrel-startup": "^1.0.0",
    "express": "^4.17.1",
    "fast-xml-parser": "^4.0.2",
    "fp-ts": "^2.11.8",
    "lodash": "^4.17.21",
    "sequelize": "^6.16.1",
    "socket.io": "^4.4.1",
    "sqlite3": "^5.0.2",
    "ws": "^8.5.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.63",
    "@electron-forge/maker-deb": "^6.0.0-beta.63",
    "@electron-forge/maker-rpm": "^6.0.0-beta.63",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.63",
    "@electron-forge/maker-zip": "^6.0.0-beta.63",
    "@types/chai": "^4.3.0",
    "@types/express": "^4.17.13",
    "@types/lodash": "^4.14.178",
    "@types/mocha": "^9.1.0",
    "@types/sinon": "^10.0.10",
    "@types/sinon-chai": "^3.2.8",
    "@types/supertest": "^2.0.11",
    "@types/validator": "^13.7.1",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "chai": "^4.3.6",
    "electron": "^16.0.9",
    "eslint": "^7.32.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-react": "^7.28.0",
    "mocha": "^9.2.0",
    "node-gyp": "^9.0.0",
    "sinon": "^13.0.1",
    "sinon-chai": "^3.7.0",
    "socket.io-client": "^4.4.1",
    "supertest": "^6.2.2",
    "ts-mocha": "^9.0.2",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.2"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "church_presenter"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}
